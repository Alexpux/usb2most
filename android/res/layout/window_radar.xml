<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="#00000000"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<ImageView
		android:background="@drawable/radar"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="0.0px"
		android:layout_marginTop="0.0px" />

	<ImageView
		android:background="@drawable/car"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="0.0px"
		android:layout_marginTop="0.0px" />

	<!-- Radar -->
	<ImageView
		android:id="@+id/front_left_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="15.0px"
		android:layout_marginTop="10.0px" />

	<ImageView
		android:id="@+id/front_left_mid_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="48.0px"
		android:layout_marginTop="0.0px" />

	<ImageView
		android:id="@+id/front_right_mid_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="100.0px"
		android:layout_marginTop="0.0px" />
    
	<ImageView
		android:id="@+id/front_right_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="124.0px"
		android:layout_marginTop="10.0px" />
    
	<ImageView
		android:id="@+id/back_left_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="17.0px"
		android:layout_marginTop="310.0px" />
    
	<ImageView
		android:id="@+id/back_left_mid_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="47.0px"
		android:layout_marginTop="316.0px" />
    
	<ImageView
		android:id="@+id/back_right_mid_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="100.0px"
		android:layout_marginTop="316.0px" />
    
	<ImageView
		android:id="@+id/back_right_img"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="124.0px"
		android:layout_marginTop="310.0px" />

</RelativeLayout>

